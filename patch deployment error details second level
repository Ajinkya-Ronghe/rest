WITH expanded_data AS (
    SELECT
        computer_name,
        custom_tags,
        operating_system,
        ipv4_address,
        tanium_client_core_health,
        patch_scan_errors::jsonb AS patch_scan_error_json,
        patch_deployment_errors::jsonb AS patch_deployment_error_json,
        patch_scan_error_json ->> 'Scan Error Message' AS patch_scan_error_message,
        patch_deployment_error_json ->> 'Error Message' AS deployment_error_message,
        patch_deployment_error_json ->> 'Deployment ID' AS deployment_id,
        tanium_client_core_health AS tanium_client_core_health,
        patch_is_process_running AS is_process_running,
        days_since_successful_scan AS days_since_successful_scan,
        staging_id,
        batch_id,
        created_time,
        bank_id
    FROM esh_main.ceh_tn_tanium_core_and_patch_health
),
latest_values AS (
    SELECT
        custom_tags,
        MAX(operating_system) AS operating_system,
        MAX(ipv4_address) AS ipv4_address,
        MAX(tanium_client_core_health) AS tanium_client_core_health,
        MAX(is_process_running) AS is_process_running,
        MAX(days_since_successful_scan) AS days_since_successful_scan,
        MAX(deployment_error_message) AS deployment_error_message,
        MAX(deployment_id) AS deployment_id,
        MAX(patch_scan_error_message) AS patch_scan_error_message,
        computer_name
    FROM expanded_data
    GROUP BY computer_name, custom_tags
)
SELECT
    *
FROM latest_values
ORDER BY deployment_error_message DESC;
