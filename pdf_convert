convertToPDF(data: any[]): Blob {
  // Create a new jsPDF instance
  const pdf = new jsPDF();

  // Extract column headers (keys from the first object)
  const headers = [Object.keys(data[0])];

  // Extract row data (values from each object)
  const rows = data.map(row => Object.values(row));

  // Add a title
  pdf.text('Exported Data', 10, 10);

  // Generate the table using autoTable
  (pdf as any).autoTable({
    head: headers,
    body: rows,
    startY: 20, // Start position after title
  });

  // Return the PDF content as a Blob
  return pdf.output('blob');
}
