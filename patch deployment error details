WITH filtered_data AS (
    SELECT
        computer_name,
        custom_tags,
        operating_system,
        ipv4_address,
        json_array_elements(patch_scan_errors::jsonb) AS patch_scan_error, -- Expand JSON array into rows
        json_array_elements(patch_deployment_errors::jsonb) AS patch_deployment_error, -- Expand JSON array into rows
        tanium_client_core_health,
        patch_is_process_running,
        days_since_successful_scan,
        staging_id,
        batch_id,
        created_time,
        bank_id
    FROM esh_main.ceh_tanium_core_and_patch_health
    WHERE patch_deployment_errors::jsonb IS NOT NULL -- Ensure the field is not null
),
processed_data AS (
    SELECT
        computer_name,
        custom_tags,
        operating_system,
        ipv4_address,
        patch_scan_error->>'Scan Error Message' AS "Scan_Error_Message",
        patch_scan_error->>'Days Since Successful Scan' AS "Days_Since_Successful_Scan",
        patch_deployment_error->>'Error Message' AS "Deployment_Error_Message",
        patch_deployment_error->>'Deployment ID' AS "Deployment_ID",
        patch_deployment_error->>'Detail' AS "Detail",
        tanium_client_core_health,
        patch_is_process_running,
        staging_id,
        batch_id,
        created_time,
        bank_id
    FROM filtered_data
    WHERE patch_deployment_error->>'Error Message' != 'No Deployment Error Message'
),
latest_values AS (
    SELECT
        custom_tags,
        MAX(operating_system) AS operating_system,
        MAX(ipv4_address) AS ipv4_address,
        MAX("Deployment_Error_Message") AS "Deployment_Error_Message",
        MAX("Deployment_ID") AS "Deployment_ID",
        MAX("Days_Since_Successful_Scan") AS "Days_Since_Successful_Scan",
        MAX("Detail") AS "Detail",
        MAX("Scan_Error_Message") AS "Scan_Error_Message",
        computer_name
    FROM processed_data
    GROUP BY computer_name, custom_tags
)
SELECT
    COUNT(*) AS count,
    "Deployment_Error_Message"
FROM latest_values
GROUP BY "Deployment_Error_Message"
ORDER BY count DESC;
